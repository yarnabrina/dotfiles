{
    "[python]": {
        "editor.defaultFormatter": "ms-python.black-formatter"
    },
    "autoDocstring.docstringFormat": "numpy",
    "black-formatter.args": [
        "--line-length",
        "99",
        "--safe"
    ],
    "black-formatter.importStrategy": "fromEnvironment",
    "comments.openView": "file",
    "csv-preview.capitalizeHeaders": false,
    "csv-preview.skipComments": true,
    "debug.allowBreakpointsEverywhere": true,
    "debug.console.closeOnEnd": true,
    "debug.saveBeforeStart": "nonUntitledEditorsInActiveGroup",
    "debug.showBreakpointsInOverviewRuler": true,
    "debug.showInStatusBar": "always",
    "debug.terminal.clearBeforeReusing": true,
    "editor.accessibilitySupport": "off",
    "editor.bracketPairColorization.independentColorPoolPerBracketType": true,
    "editor.copyWithSyntaxHighlighting": false,
    "editor.dragAndDrop": false,
    "editor.emptySelectionClipboard": false,
    "editor.find.autoFindInSelection": "multiline",
    "editor.find.cursorMoveOnType": false,
    "editor.find.seedSearchStringFromSelection": "selection",
    "editor.fontFamily": "Source Code Pro",
    "editor.fontLigatures": true,
    "editor.fontSize": 20,
    "editor.guides.bracketPairs": true,
    "editor.linkedEditing": true,
    "editor.matchBrackets": "near",
    "editor.maxTokenizationLineLength": 5000,
    "editor.minimap.enabled": false,
    "editor.renderWhitespace": "boundary",
    "editor.rulers": [
        {
            "color": "#00FF00",
            "column": 72
        },
        {
            "color": "#0000FF",
            "column": 88
        },
        {
            "color": "#FF0000",
            "column": 99
        }
    ],
    "editor.suggest.localityBonus": true,
    "editor.suggest.preview": true,
    "editor.wordWrap": "bounded",
    "editor.wordWrapColumn": 120,
    "editor.wrappingIndent": "indent",
    "explorer.autoReveal": false,
    "explorer.confirmDelete": false,
    "explorer.confirmDragAndDrop": false,
    "explorer.fileNesting.enabled": true,
    "explorer.incrementalNaming": "smart",
    "extensions.autoUpdate": false,
    "extensions.closeExtensionDetailsOnViewChange": true,
    "extensions.ignoreRecommendations": true,
    "files.autoGuessEncoding": true,
    "flake8.args": [
        "--extend-ignore",
        "E203",
        "--per-file-ignores",
        "__init__.py:F401",
        "--max-line-length",
        "99"
    ],
    "flake8.importStrategy": "fromEnvironment",
    "git.autoRepositoryDetection": false,
    "git.autoStash": true,
    "git.branchProtectionPrompt": "alwaysCommitToNewBranch",
    "git.confirmSync": false,
    "git.pullBeforeCheckout": true,
    "git.untrackedChanges": "separate",
    "git.useCommitInputAsStashMessage": true,
    "gitlens.blame.ignoreWhitespace": true,
    "gitlens.showWelcomeOnInstall": false,
    "gitlens.showWhatsNewAfterUpgrades": false,
    "gitlens.statusBar.enabled": false,
    "gitlens.telemetry.enabled": false,
    "isort.args": [
        "--profile",
        "black",
        "--atomic",
        "--line-length",
        "99",
        "--remove-redundant-aliases"
    ],
    "isort.check": true,
    "isort.importStrategy": "fromEnvironment",
    "jupyter.askForKernelRestart": false,
    "jupyter.disableJupyterAutoStart": true,
    "jupyter.experiments.enabled": false,
    "jupyter.exportWithOutputEnabled": true,
    "jupyter.themeMatplotlibPlots": true,
    "markdown.extension.list.indentationSize": "inherit",
    "markdown.validate.enabled": true,
    "material-icon-theme.activeIconPack": "none",
    "merge-conflict.autoNavigateNextConflict.enabled": true,
    "mypy-type-checker.args": [
        "--strict",
        "--ignore-missing-imports"
    ],
    "mypy-type-checker.importStrategy": "fromEnvironment",
    "notebook.cellToolbarVisibility": "hover",
    "notebook.consolidatedRunButton": true,
    "notebook.diff.ignoreMetadata": true,
    "notebook.diff.ignoreOutputs": true,
    "notebook.dragAndDropEnabled": false,
    "notebook.lineNumbers": "on",
    "notebook.output.scrolling": true,
    "notebook.showFoldingControls": "always",
    "oneDarkPro.bold": true,
    "oneDarkPro.vivid": true,
    "partialDiff.commandsOnContextMenu": {
        "diffVisibleEditors": false,
        "togglePreComparisonTextNormalizationRules": false
    },
    "partialDiff.enableTelemetry": false,
    "path-intellisense.autoTriggerNextSuggestion": true,
    "path-intellisense.showHiddenFiles": true,
    "problems.showCurrentInStatus": true,
    "pylint.args": [
        "--enable-all-extensions",
        "--enable",
        "all",
        "--disable",
        "import-outside-toplevel",
        "logging-fstring-interpolation",
        "--logging-format-style",
        "new",
        "--max-line-length",
        "99"
    ],
    "pylint.importStrategy": "fromEnvironment",
    "python.analysis.completeFunctionParens": true,
    "python.analysis.typeCheckingMode": "basic",
    "python.experiments.enabled": false,
    "python.terminal.activateEnvInCurrentTerminal": true,
    "python.testing.pytestArgs": [
        "--doctest-modules",
        "--doctest-ignore-import-errors",
        "--doctest-continue-on-failure"
    ],
    "python.testing.pytestEnabled": true,
    "pythonIndent.trimLinesWithOnlyWhitespace": true,
    "rainbow_csv.rbql_with_headers_by_default": true,
    "redhat.telemetry.enabled": false,
    "remote.autoForwardPortsSource": "hybrid",
    "ruff.codeAction.disableRuleComment.enable": true,
    "ruff.codeAction.fixViolation.enable": true,
    "ruff.configurationPreference": "filesystemFirst",
    "ruff.fixAll": true,
    "ruff.importStrategy": "useBundled",
    "ruff.lineLength": 100,
    "ruff.lint.ignore": [
        "COM",
        "D203",
        "D213",
        "DTZ",
        "EM",
        "FBT",
        "FIX",
        "G",
        "ICN",
        "PD",
        "RET501",
        "RET502",
        "RET503",
        "RET504",
        "SLF",
        "TRY003"
    ],
    "ruff.lint.select": [
        "ALL"
    ],
    "ruff.organizeImports": false,
    "ruff.showSyntaxErrors": true,
    "scm.alwaysShowActions": true,
    "scm.alwaysShowRepositories": true,
    "scm.defaultViewMode": "tree",
    "scm.diffDecorationsGutterPattern": {
        "added": true
    },
    "scm.repositories.sortOrder": "path",
    "search.collapseResults": "auto",
    "search.defaultViewMode": "tree",
    "search.showLineNumbers": true,
    "search.smartCase": true,
    "search.useGlobalIgnoreFiles": true,
    "search.useParentIgnoreFiles": true,
    "spellright.documentTypes": [
        "latex",
        "markdown",
        "plaintext",
        "python"
    ],
    "spellright.language": [
        "en-GB"
    ],
    "telemetry.telemetryLevel": "off",
    "terminal.integrated.confirmOnExit": "hasChildProcesses",
    "terminal.integrated.confirmOnKill": "always",
    "terminal.integrated.cursorBlinking": true,
    "terminal.integrated.cursorStyle": "underline",
    "terminal.integrated.enableMultiLinePasteWarning": false,
    "terminal.integrated.fontFamily": "MesloLGS NF",
    "terminal.integrated.fontSize": 16,
    "terminal.integrated.inheritEnv": false,
    "terminal.integrated.persistentSessionReviveProcess": "never",
    "terminal.integrated.rightClickBehavior": "default",
    "update.enableWindowsBackgroundUpdates": false,
    "update.showReleaseNotes": false,
    "window.autoDetectColorScheme": false,
    "window.dialogStyle": "custom",
    "window.menuBarVisibility": "visible",
    "window.restoreWindows": "one",
    "workbench.colorTheme": "One Dark Pro",
    "workbench.commandPalette.history": 5,
    "workbench.commandPalette.preserveInput": true,
    "workbench.editor.focusRecentEditorAfterClose": false,
    "workbench.editor.openPositioning": "last",
    "workbench.editor.pinnedTabSizing": "shrink",
    "workbench.editor.preferHistoryBasedLanguageDetection": true,
    "workbench.editor.revealIfOpen": true,
    "workbench.editor.sharedViewState": true,
    "workbench.editor.splitSizing": "split",
    "workbench.editor.tabSizing": "shrink",
    "workbench.editor.titleScrollbarSizing": "large",
    "workbench.editor.wrapTabs": true,
    "workbench.enableExperiments": false,
    "workbench.iconTheme": "material-icon-theme",
    "workbench.layoutControl.type": "toggles",
    "workbench.list.defaultFindMode": "filter",
    "workbench.panel.opensMaximized": "never",
    "workbench.productIconTheme": "fluent-icons",
    "workbench.quickOpen.preserveInput": true,
    "workbench.tree.indent": 16,
    "workbench.tree.renderIndentGuides": "always",
    "workbench.welcomePage.walkthroughs.openOnInstall": false
}